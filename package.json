{
  "name": "remotion-time",
  "version": "0.0.3",
  "description": "Time-based hooks for Remotion animations.",
  "keywords": [
    "remotion",
    "animation",
    "timeline",
    "react",
    "hook"
  ],
  "license": "MIT",
  "author": "John Dunning (https://github.com/fwextensions)",
  "repository": "github:fwextensions/remotion-time",
  "homepage": "https://github.com/fwextensions/remotion-time",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/remotion-time.cjs",
  "module": "./dist/remotion-time.js",
  "exports": {
    ".": {
      "require": "./dist/remotion-time.cjs",
      "import": "./dist/remotion-time.js"
    }
  },
  "types": "./dist/remotion-time.d.ts",
  "scripts": {
    "dev": "rollup --watch",
    "build": "rollup -c",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "react": "^18",
    "remotion": "^4"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "esbuild": "^0.19.2",
    "rollup": "^3.28.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0"
  }
}
